        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Console Input
if (global.typing)
{
    var cmd = parse(keyboard_string);
    switch (cmd)
    {
        case "help":
            cout("Commands:");
            var key = ds_map_find_first(commands);
            for (var i = 0; i &lt; ds_map_size(commands); i++)
            {
                value = ds_map_find_value(commands, key);
                cout("-" + key + string_replace(string(value), ",", " "));
                key = ds_map_find_next(commands, key);
            }
            break;
        case "server":
            var server = instance_create(0, 0, o_server);
            server.max_clients = round(real(args[1]));
            cout("Server created with " + string(server.max_clients) + " max clients");
            break;
        case "connect":
            var client = instance_create(0, 0, o_client);
            client.host = split(args[1], ":");
            break;
        case "disconnect":
            if (instance_exists(o_client))
                o_client.disconnect = true;
            disconnect = true;
            break;
        case "invalid":
        case "misuse":
            // these are handled in parse
            break;
        case "say":
            if (!isEmpty(keyboard_string))
                cout(o_player.name + ": " + keyboard_string);
            break;
    }

    // clear and stop typing
    keyboard_string = "";
    global.typing = false;
}
else
{
    // clear and start typing
    keyboard_string = "";
    global.typing = true;
}
</string>
