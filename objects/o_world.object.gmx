        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Toggle Typing
if (global.typing)
{
    // say
    switch (parse(keyboard_string))
    {
        case "help":
            cout("Commands:");
            key = ds_map_find_first(commands);
            for (var i = 0; i &lt; ds_map_size(commands); i++)
            {
                value = ds_map_find_value(commands, key);
                cout("-" + key + string_replace(string(value), ",", " "));
                key = ds_map_find_next(commands, key);
            }
            break;
        case "server":
            server = instance_create(0, 0, o_server);
            server.max_clients = round(real(args[1]));
            cout("Server created with " + string(server.max_clients) + " max clients");
            break;
        case "connect":
            client = instance_create(0, 0, o_client);
            client.host = split(args[1], ":");
            break;
        case "disconnect":
            with(instance_nearest(0, 0, o_client))
            {
                disconnect = true;
            }
            break;
        case "invalid":
            cout("Invalid command: " + cmd)
            break;
        case "misuse":
            cout("Usage:");
            cout("-" + cmd + string_replace(ds_map_find_value(commands, cmd), ",", " "));
            break;
        case "say":
            cout(o_player.name + ": " + keyboard_string);
            break;
    }

    // clear and stop typing
    keyboard_string = "";
    global.typing = false;
    args = 0;
    cmd = "";
}
else
{
    // clear and start typing
    keyboard_string = "";
    global.typing = true;
}
</string>
